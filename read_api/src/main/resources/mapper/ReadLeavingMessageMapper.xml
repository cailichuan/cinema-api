<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cinema.read_api.mapper.ReadLeavingMessageMapper">

    <select id="selectList" resultType="LeavingMessage">
        select *
        from cinema.t_leaving_message
    </select>

    <select id="selectListByUid" parameterType="Integer" resultType="LeavingMessage">
        select *
        from cinema.t_leaving_message
        where uid = #{uid}
    </select>
</mapper>